.photo-view{
  width: 100%;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.takePhoto{
  width: 100rpx;
  height: 100rpx;
  border-radius: 100rpx; 
  background-color: #fafffd;
  color: white;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.result_pic{
  width: 100rpx;
  height: 100rpx;
}
.font-size{
  font-size: 35rpx;
  font-weight: 600;
}